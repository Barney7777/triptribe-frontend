{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "indent": ["error", 2, { "ignoredNodes": ["ConditionalExpression *"], "SwitchCase": 1 }],
    "quotes": ["error", "single"],
    "no-var": "error",
    "no-unused-vars": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
